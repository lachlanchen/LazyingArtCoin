# Sepolia RPC endpoint for testing LazyArtCoin
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_PROJECT_ID

# Private key of the deployer wallet (MetaMask test account)
DEPLOYER_PRIVATE_KEY=0xYOUR_PRIVATE_KEY

# Optional: address that should own the contract after deploy (defaults to deployer)
OWNER_ADDRESS=0xYOUR_OWNER_ADDRESS

# Optional: treasury wallet receiving the initial mint (defaults to owner)
TREASURY_ADDRESS=0xYOUR_TREASURY_ADDRESS

# Optional: initial supply in whole LAC to mint during deployment
INITIAL_SUPPLY=1000000

# Optional: Etherscan API key for Sepolia verification
ETHERSCAN_API_KEY=YOUR_ETHERSCAN_KEY
